/*############ 초기화 #################*/
	*{margin:0;padding:0;}
	li{list-style-type:none;}
	a:link,a:visited{text-decoration:none;color:#fff;}
	a:hover,a:focus{color:#DC2E8C;}
	body{font-family:맑은고딕,"Malgun Gothic",돋움,Dotum,Tahoma,Geneva,sans-serif;}
	h2{position:absolute;left:-9999px;width:1px;height:1px;font-size:1px;
			line-height:0;overflow:hidden;}
	h4{position:absolute;left:-9999px;width:1px;height:1px;font-size:1px;
			line-height:0;overflow:hidden;}