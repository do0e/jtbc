		.cImg{width:512px;height:925px;margin-top:50px;} .cImg img{width:100%;height:100%;}
		.cText{width:593px;margin:0 30px 0 100px;}
		.cDesc{width:100%;margin:50px 0 30px 0;}
		.cDesc strong{display:block;width:100%;font-size:18px;margin-bottom:10px;}
		.cDesc span{display:block;width:95%;margin-bottom:7px;color:#666;}

