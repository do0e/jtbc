				#header #navWrap #gnb li a{font-size:18px;line-height:70px;
											font-weight:600;
											padding:2px 17px;color:#333;}

				#header #navWrap #gnb li a:hover{color:#DC2E8C;}
				#header #navWrap #searchs p{float:left; width:30px;height:70px; 
											margin:20px 0;}
				#header #navWrap #searchs p img{width:100%;}
				#header #navWrap #searchs p.search2{display:none;}

			/*############# navWrap : hover #############*/
			#header #navWrap div#searchs:hover p.search1{display:none;}
			#header #navWrap div#searchs:hover p.search2{display:block;}

	/* ############ container ############## */
		#container{width:100%;margin-top:30px;overflow:hidden;}
		/* ###### any Head ####### */
		#anyHead{width:100%;}
		#anyHead ul{width:250px;height:70px;
				   margin:30px auto 0;overflow:hidden;position:relative;}
			#anyHead li{list-style-type:none;text-align:center;font-weight:900;
				  width:100px;height:30px;font-size:20px;position:absolute;top:0;
				  text-shadow:2px 2px 5px rgba(0,0,0,0.1);color:#555;}
			.j{left:0;animation:aniText 1s 1;}
			.t{left:30px;animation:aniText 1.5s 1;}
			.b{left:60px;animation:aniText 2s 1;}
			.c{left:90px;animation:aniText 2.5s 1;}
			.s{left:120px;animation:aniText 3s 1;}
			.g{left:150px;animation:aniText 3.5s 1;}
	@keyframes aniText{
		0%{top:-30px;}
		70%{top:30px;text-transform:lowercase;}
		100%{top:0;}
	}
	/* ######### companyVision ######### */
		.companyVision{width:1245px;margin:0 auto;overflow:hidden;}
		.vision,.mission{width:590px;height:303px;float:left;}
		.vision{margin-right:65px;}
		.vTitle{width:100%;font-size:20px;padding-bottom:5px;border-bottom:2px solid #333;
				font-weight:bold;}
		.pbold{width:325px;height:73px;font-size:25px;margin:30px 0;font-weight:bold;}
		.vDesc{width:100%;color:#666;}

		.jtbcBanner{width:1245px;margin:20px auto;} .jtbcBanner img{width:100%;}

	/* ########### companDesc ############ */
		.companyDesc{width:1245px;margin:0 auto;overflow:hidden;}
		.cTitle{width:100%;font-size:20px;padding-bottom:5px;border-bottom:2px solid #333;
