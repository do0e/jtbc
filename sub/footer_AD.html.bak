  <script>
	$(function(){

		//초기화
		//마지막 장면 미리 대기시켜 놓기
		$("#film").prepend($(".scene:last"));
		$("#film").css({"marginLeft":"-1920px"});
		
		/* 다음버튼 */
		$(".nextBtn").click(function(){
			$("#film:not(:animated)").stop().animate({"marginLeft":"-=1920px"},800,"linear",function(){
				$("#film").append($(".scene:first"));
				$("#film").css({"marginLeft":"-1920px"});
			});
		});
		
		
		/* 이전버튼 */

		$(".prevBtn").click(function(){
			$("#film:not(:animated)").stop().animate({"marginLeft":"+=1920px"},800,"linear",function(){
				
			$("#film").prepend($(".scene:last"));
			$("#film").css({"marginLeft":"-1920px"});
			
			});
		});
	
	});
