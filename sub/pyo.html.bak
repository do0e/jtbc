  <script>
	$(function(){
		
		//마우스 박스
		$("#pyoList li").mouseover(function(){
			$(".pyoBack").css({"width":$(this).width(),"height":$(this).height(),"left":$(this).offset().left,"top":$(this).offset().top,"fontSize":"16px","lineHeight":$(this).height()+"px","color":"rgba(255,255,255,1)"});
		});
		$("#pyoList li.last").mouseover(function(){
			$(".pyoBack").css({"width":"1px","height":"1px","fontSize":"1px","backgroundColor":"rgba,(232,109,3,0.2)","lineHeight":"1px","color":"rgba(0,0,0,0)"});
		});
	
		//다음버튼
		
		$(".nextArrow").click(function(){
			$(".pyoDate").animate({"marginLeft":"-=102px"},1000);
		});
		
		//이전버튼
		$(".prevArrow").click(function(){
			$(".pyoDate").animate({"marginLeft":"+=92px"},1000);
		});
		
	
		//noservice
		$("#nodesc").hide();
		
		//클릭 카운트 변수 값 지정
		var count=1;

		$(".scene").click(function(){
			$(".pyoDate li a").removeClass("pyoSelected");
			$(this).children("a").addClass("pyoSelected");
			$("#pyoList").hide();
			$("#nodesc").show();
			
			//클릭 카운트 ++; 이미지 01~04까지므로 클릭카운트 5가 되면 다시 1로 돌아오게 지정
				count++;
			if(count == 5){ count=1; }
			$("#noicon").html("<img src='img/ico_no_service0"+count+".png' alt='이미지'/>");
		});

		$(".scene:first").click(function(){
			$("#pyoList").show();
			$("#nodesc").hide();
		});

		
	});
